; Enable and disable various interrupts
PROTOTYPE SUB		ONKEY(i,mask,flag)
PROTOTYPE SUB		ONTIMER(i)
PROTOTYPE SUB		ONHBLANK(i)
PROTOTYPE SUB		ONVBLANK(i)
PROTOTYPE SUB		ONVCOUNT(i)
